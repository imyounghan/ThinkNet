<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="thinkcfg.log_appender" value="console"/>
    <add key="thinkcfg.log_priority" value="debug"/>
    <add key="thinkcfg.kafka_uri" value="tcp://127.0.0.1:9092"/>
    <add key="thinkcfg.kafka_topic" value="CommandResults"/>
    <add key="thinkcfg.zookeeper_address" value="127.0.0.1:2181"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
